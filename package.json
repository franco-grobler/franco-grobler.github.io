{
  "name": "franco-grobler-website",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest",
    "lint": "eslint --fix app/",
    "format": "prettier --write .",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:unit": "vitest --project unit",
    "test:nuxt": "vitest --project nuxt",
    "test:e2e": "NITRO_PRESET=node-server NODE_OPTIONS='--max-old-space-size=4096' npx playwright test",
    "test:e2e:ui": "NITRO_PRESET=node-server NODE_OPTIONS='--max-old-space-size=4096' npx playwright test --ui"
  },
  "dependencies": {
    "@nuxt/content": "3.11.2",
    "@nuxt/eslint": "1.15.1",
    "@nuxt/hints": "1.0.0-alpha.6",
    "@nuxt/image": "2.0.0",
    "@nuxt/test-utils": "4.0.0",
    "@nuxt/ui": "4.4.0",
    "better-sqlite3": "^12.5.0",
    "eslint": "^10.0.0",
    "nuxt": "^4.3.1",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.6.3",
    "vue": "^3.5.28",
    "vue-router": "^5.0.2"
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.2.90",
    "@iconify-json/material-symbols": "^1.2.55",
    "@iconify-json/simple-icons": "^1.2.70",
    "@playwright/test": "^1.58.2",
    "@tiptap/extension-drag-handle": "^3.19.0",
    "@vitest/coverage-v8": "^4.0.16",
    "@vue/test-utils": "^2.4.6",
    "happy-dom": "^20.6.1",
    "playwright-core": "^1.58.2",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "vitest": "^4.0.16",
    "zod": "^4.3.4",
    "zod-to-json-schema": "^3.25.1"
  },
  "packageManager": "bun@1.3.6"
}
